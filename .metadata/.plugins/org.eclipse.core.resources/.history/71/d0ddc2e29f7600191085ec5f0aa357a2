package Web;

public class ProgressaoBean {
	
	private Double a1;
	private Integer n;
	private Double an;
	private Double r;
	
	public void Calcular () {
		
		
		
		
	}
	
	public Double getA1() {
		return a1;
	}
	public void setA1(Double a1) {
		this.a1 = a1;
	}
	public Integer getN() {
		return n;
	}
	public void setN(Integer n) {
		this.n = n;
	}
	public Double getAn() {
		return an;
	}
	public void setAn(Double an) {
		this.an = an;
	}
	public Double getR() {
		return r;
	}
	public void setR(Double r) {
		this.r = r;
	}
	

}


